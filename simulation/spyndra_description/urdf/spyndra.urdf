  <joint name="base_to_femur_1" type="revolute">
    <origin rpy="0 0 0" xyz="0.6 0 0.1"/>
    <axis xyz="0 1 0"/>
    <limit effort="50.0" lower="-2.0" upper="2.0" velocity="0.1"/>
    <parent link="base_link"/>
    <child link="femur_1"/>
  </joint>

  <joint name="base_to_femur_2" type="revolute">
